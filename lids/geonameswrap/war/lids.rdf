<?xml version="1.0" encoding="utf-8"?>
<rdf:RDF xmlns:lids="http://openlids.org/vocab#" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
  <rdf:Description rdf:about="">
    <rdfs:label>OpenLIDS GeoNames Service Description</rdfs:label>
  </rdf:Description>

  <rdf:Description rdf:about="#findNearby">
    <lids:description>PREFIX foaf: &lt;http://xmlns.com/foaf/0.1/&gt;
PREFIX geo: &lt;http://www.w3.org/2003/01/geo/wgs84_pos#&gt;
CONSTRUCT {
   ?point foaf:based_near ?feature .
}
FROM &lt;http://geowrap.openlids.org/findNearby&gt;
WHERE {
   ?point geo:lat ?lat . ?point geo:long ?lng .
}</lids:description>
  </rdf:Description>

  <rdf:Description rdf:about="#findNearbyPlaceName">
    <lids:description>PREFIX foaf: &lt;http://xmlns.com/foaf/0.1/&gt;
PREFIX geo: &lt;http://www.w3.org/2003/01/geo/wgs84_pos#&gt;
CONSTRUCT {
   ?point foaf:based_near ?feature .
}
FROM &lt;http://geowrap.openlids.org/findNearbyPlaceName&gt;
WHERE {
   ?point geo:lat ?lat . ?point geo:long ?lng .
}</lids:description>
  </rdf:Description>
  
    <rdf:Description rdf:about="#location">
    <lids:description>PREFIX owl: &lt;http://www.w3.org/2002/07/owl#&gt;
PREFIX geo: &lt;http://www.w3.org/2003/01/geo/wgs84_pos#&gt;
CONSTRUCT {
   ?point owl:sameAs ?location .
}
FROM &lt;http://geowrap.openlids.org/loc&gt;
WHERE {
   ?point geo:lat ?lat . ?point geo:long ?lng .
}</lids:description>
  </rdf:Description>
</rdf:RDF>
