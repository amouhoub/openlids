PREFIX foaf: <http://xmlns.com/foaf/0.1/>
PREFIX cb: <http://ontologycentral.com/2010/05/cb/vocab#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>

CONSTRUCT { ?s cb:jobs ?id . } 
FROM <http://jobswrap.ontologycentral.com/jobs> 
WHERE { 
	?s cb:name ?name .
	?s cb:office ?office .
    ?office cb:city ?location .
}
